import java.util.Scanner;

public class uri_1012 {

	public static void main(String[] args) {
		
	Scanner sc = new Scanner(System.in);
	
		double altura;
		double base;
		double lado; 
		double raio;
		double pi = 3.14159;
		
		altura = sc.nextDouble();
		base = sc.nextDouble();
		lado = sc.nextDouble();
		raio = sc.nextDouble();
		
		double triangulo = base * altura /2;
		double circulo = raio * Math.pow(pi, 2);
		double trapezio = (base + base) * altura /2;
		double quadrado = Math.pow(lado, 2);
		double retangulo = base * altura;
		
		System.out.println("TRIANGULO: " + triangulo);
		System.out.println("CIRCULO: " + circulo);
		System.out.println("TRAPEZIO: " + trapezio);
		System.out.println("QUADRADO: " + quadrado);
		System.out.println("RETANGULO: " + retangulo);
	
	}

}
